
@{
    ViewData["Title"] = "StudentList";
}

<h1>StudentList</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Student s in (IEnumerable<Student>)ViewData["list"])
        {
            <tr>
                <td>@s.Rollno</td>
                <td>@s.Name</td>
                <td>@s.City</td>
            </tr>
        }
    </tbody>
</table>


