
@{
    ViewData["Title"] = "EmpList";
}

<h1>EmpList</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Dept</th>
            <th>PhoneNumber</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Employee e in (IEnumerable<Employee>)ViewData["list"])
        {
            <tr>
                <td>@e.Id</td>
                <td>@e.Name</td>
                <td>@e.Dept</td>
                <td>@e.PhoneNumber</td>
            </tr>
        }
    </tbody>
</table>



